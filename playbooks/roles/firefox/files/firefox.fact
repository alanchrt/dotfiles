#!/usr/bin/env bash

current_version=$(apt-cache policy firefox | awk 'NR == 2' | awk '{print $2}')
available_version=$(apt-cache policy firefox | awk 'NR == 3' | awk '{print $2}')

if [ "$current_version" != "$available_version" ]; then
  out_of_date=true
else
  out_of_date=false
fi

echo "{\"out_of_date\": $out_of_date}"